#include "jank.ebnf" ;
#include "function.ebnf" ;

(* math expressions *)
(* currently, + - (multiply) / are supported *)
(* TODO support more operations *)

expr_factor = [ "+" , ows | "-" , ows ] , ( function_call | identifier | number | "(" , ows , expression , ows , ")" ) ;
expr_term = expr_factor , { ows , ( "*" | "/" ) , ows , expr_factor } ;
expression = expr_term , { ows , ( "+" | "-" ) , ows , expr_term } ;