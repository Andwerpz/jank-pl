#include <iostream>;

i32 main(u64 argc, u8** argv) {
    
    cout << "ARGC : " << argc << "\n";
    for(u64 i = 0x0; i < argc; i++) {
        cout << "ARGV : " << argv[i] << "\n";
    }

    u64 ptr = 0x0;
    while($void* environ[ptr] != nullptr) {
        cout << "ENVIRON : " << environ[ptr] << "\n";
        ptr ++;
    }

    return 0;
}