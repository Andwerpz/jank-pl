
int main() {

    char* hello_str = malloc(6 * sizeof(char));
    hello_str[0] = 'h';
    hello_str[1] = 'e';
    hello_str[2] = 'l';
    hello_str[3] = 'l';
    hello_str[4] = 'o';
    hello_str[5] = '\0';

    char* space_str = malloc(2 * sizeof(char));
    space_str[0] = ' ';
    space_str[1] = '\0';

    char* endl_str = malloc(2 * sizeof(char));
    endl_str[0] = '\n';
    endl_str[1] = '\0';

    puts_endl(hello_str);

    int N = 10;
    int** dp = malloc(N * sizeof(int*));
    for(int i = 0; i < N; i = i + 1) dp[i] = malloc(N * sizeof(int));
    dp[0][0] = 1;
    for(int i = 0; i < N; i = i + 1) {
        for(int j = 0; j < N; j = j + 1) {
            if(i != 0) dp[i][j] = dp[i][j] + dp[i - 1][j];
            if(j != 0) dp[i][j] = dp[i][j] + dp[i][j - 1];
        }
    }
    for(int i = 0; i < N; i = i + 1) {
        for(int j = 0; j < N; j = j + 1) {
            puti(dp[i][j]);
            puts(space_str);
        }
        puts(endl_str);
    }

    return 0;
}