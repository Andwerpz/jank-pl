#include <put>;
#include <jstring>;
#include <vector>;
#include <iostream>;
#include <assert>;

struct A {
    i32 foo;

    A(i32 _foo) {
        this.foo = _foo;
    }
}

A operator+(A& x, A& y) {
    cout << "operator+(A&, A&)\n";
    return new A(x.foo + y.foo);
}

A operator+(A x, A y) {
    cout << "operator+(A, A)\n";
    return new A(x.foo + y.foo);
}

i32 main() {

    A x = new A();
    A y = new A();
    A a = x + y;                // operator+(A&, A&)
    A b = new A() + new A();    // operator+(A, A)
    A c = x + new A();          // operator+(A, A)

    return 0;
}