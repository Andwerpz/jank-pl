#include <jstring>;
#include <put>;
#include <assert>;
#include <iostream>;

struct vec2 {
    i32 x;
    i32 y;

    vec2() {
        this.x = 0;
        this.y = 0;
    }
    vec2(vec2& other) {
        this.x = other.x;
        this.y = other.y;
    }
    vec2(i32 x, i32 y) {
        this.x = x;
        this.y = y;
    }
    ~vec2() {}

    void print() {
        cout << "(" << this.x << ", " << this.y << ")\n";
    }
}

vec2 operator+(vec2 a, vec2 b) {
    return new vec2(a.x + b.x, a.y + b.y);
}

vec2 operator-(vec2 a, vec2 b) {
    return new vec2(a.x - b.x, a.y - b.y);
}

vec2 operator*(vec2 a, i32 s) {
    return new vec2(a.x * s, a.y * s);
}

vec2 operator/(vec2 a, i32 s) {
    return new vec2(a.x / s, a.y / s);
}

i32 operator==(vec2 a, vec2 b) {
    return a.x == b.x && a.y == b.y;
}

ostream& operator<<(ostream& stream, vec2 v) {
    return stream << "(" << v.x << ", " << v.y << ")";
}

void vec2_test() {
    {
        vec2 a = new vec2(1, 2);
        vec2 b = new vec2(3, 4);
        vec2 c = a + b;
        c.print();
        assert(c == new vec2(4, 6));
    }

    cout << "vec2 test passed" << "\n";
}

void ostream_test() {

    cout << "bello!!" << "\n";
    i32 x = 100;
    cout << "X : " << x << "\n";

    vec2 a = new vec2(69, 420);
    cout << "A : " << a << "\n";

    cout << "NESTING TEST : " << "\n";
    cout << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "." << "\n";

    cout << "ostream test passed" << "\n";
}

i32 main() {

    vec2_test();
    ostream_test();
    cout << "all tests passed" << "\n";

    return 0;
}