

//merge sort
void sort(int* a, int n) {
    if(n == 1) {
        return;
    }
    int lsz = n / 2;
    int rsz = n - lsz;
    int* l = malloc(lsz * sizeof(int));
    int* r = malloc(rsz * sizeof(int));
    for(int i = 0; i < lsz; i = i + 1) {
        l[i] = a[i];
    }
    for(int i = 0; i < rsz; i = i + 1){
        r[i] = a[i + lsz];
    }
    sort(l, lsz);
    sort(r, rsz);
    int lptr = 0;
    int rptr = 0;
    for(int i = 0; i < n; i = i + 1) {
        if(rptr == rsz) {
            a[i] = l[lptr];
            lptr = lptr + 1;
        }
        else if(lptr == lsz) {
            a[i] = r[rptr];
            rptr = rptr + 1;
        }
        else if(l[lptr] < r[rptr]) {
            a[i] = l[lptr];
            lptr = lptr + 1;
        }
        else {
            a[i] = r[rptr];
            rptr = rptr + 1;
        }
    }
}

int main() {
    int N = 1000;
    int* a = malloc(N * sizeof(int));
    
    //pseudorandom array
    {
        int mod = 100007;
        int ptr = 1;
        int mult = 67;
        int add = 103;
        for(int i = 0; i < N; i = i + 1) {
            ptr = (ptr * mult + add) % mod;
            a[i] = ptr;
        }
    }
    
    sort(a, N);
    for(int i = 0; i < N; i = i + 1) {
        puti_endl(a[i]);
    }

    return 0;
}