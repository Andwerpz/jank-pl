#include <iostream>;
#include <assert>;

// 1/19/2026

//was getting floating point errors when trying to divide unsigned types with their highest bit set. 
//turns out I was using 'cqo' to sign extend the unsigned values when dividing :/
//also need to make sure to clear %rdx before dividing as that's the upper part of the numerator

i32 main() {
    {
        u32 a = $u32 0x80000000;
        u32 b = $u32 1;
        u32 c = a / b;
        cout << "ABC : " << a << " " << b << " " << c << "\n";
    }

    {
        u16 a = $u16 0x8000;
        u16 b = $u16 1;
        u16 c = a / b;
        cout << "ABC : " << a << " " << b << " " << c << "\n";
    }

    {
        u8 a = $u8 0x80;
        u8 b = $u8 1;
        u8 c = a / b;
        cout << "ABC : " << $u16 a << " " << $u16 b << " " << $u16 c << "\n";
    }

    return 0;
}