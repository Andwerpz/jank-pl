
void assert(i64 x) {
    if(!x) {
        puts_endl("assert failed");
        sys_exit(1);
    }
}

struct ostream {
    ostream() {
        // do nothing
    }
    ostream(ostream& other) {
        // do nothing
    }
}

ostream& operator<<(ostream& stream, i64 x) {
    puti(x);
    return stream;
}

ostream& operator<<(ostream& stream, u8* s) {
    puts(s);
    return stream;
}

template<T>
struct vector {
    i64 cap;
    i64 sz;
    T* arr;

    vector() {
        this.cap = 1;
        this.sz = 0;
        this.arr = (T*) malloc(sizeof(T) * 1);
    }
    vector(vector& other) {
        this.cap = other.cap;
        this.sz = other.sz;
        this.arr = (T*) malloc(sizeof(T) * this.cap);
        for(i64 i = 0; i < this.cap; i++) this.arr[i] = other.arr[i];
    }
    vector(i64 n, T default) {
        this.cap = n;
        this.sz = n;
        this.arr = (T*) malloc(sizeof(T) * n);
        for(i64 i = 0; i < n; i++) this.arr[i] = default;
    }
}

i64& operator[](vector<i64>& a, i64 ind) {
    return a.arr[ind];
}

vector<i64>& operator[](vector<vector<i64>>& a, i64 ind) {
    return a.arr[ind];
}

i64 main() {

    i64 n = 10;
    ostream cout = new ostream();
    vector<vector<i64>> dp = new vector<vector<i64>>(n, new vector<i64>(n, 0));

    dp[0][0] = 1;
    for(i64 i = 0; i < n; i++){
        for(i64 j = 0; j < n; j++){
            if(i != 0) dp[i][j] += dp[i - 1][j];
            if(j != 0) dp[i][j] += dp[i][j - 1];
        }
    }
    for(i64 i = 0; i < n; i++){
        for(i64 j = 0; j < n; j++){
            cout << dp[i][j] << " ";
        }
        cout << "\n";
    }

    return 0;
}