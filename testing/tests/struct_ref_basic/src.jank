#include <jstring>;
#include <put>;

struct A {
    i32 val;

    A() {
        this.val = new i32();
    }
    A(A& other) {
        this.val = other.val;
    }
    A(i32 _val) {
        this.val = _val;
    }
}

i32 main() {

    i32 n = 10;
    A* arr = (A*) malloc(sizeof(A) * (u64) n);
    for(i32 i = 0; i < n; i++){
        arr[i] = new A(i);
    }

    A& ref = arr[1];
    ref.val = 123;

    A* ptr = (A*) malloc(sizeof(A));
    *ptr = new A(100);
    arr[0] = *ptr;
    for(i32 i = 0; i < n; i++){
        puti_endl(arr[i].val);
    }

    return 0;
}