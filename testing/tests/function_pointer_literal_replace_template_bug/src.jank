//replace templated type inside function pointer literal bug
//was failing to replace templated types contained on surface level of function pointer literal

typedef i64 ll;
typedef i32 int;

i32 ll_cmp(ll a, ll b) {
    return a < b;
}

template<T>
i32 do_cmp(T& a, T& b, fn<i32(T&, T&)> cmp) {
    return cmp#(a, b);
}

i32 main() {

    fn<i32(ll, ll)> cmp = #<ll_cmp(ll, ll)>;
    assert(cmp#($ll 1, $ll 2) == 1, "1 < 2");

    cout << "test passed\n";

    return 0;
}