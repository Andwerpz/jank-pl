#include <iostream>;
#include <random>;
#include <assert>;
#include <string>;

u8* gen_trash(u64 sz) {
    u8* buf = $u8* malloc(sz);
    for(u64 i = 0x0; i < sz; i++) buf[i] = rand_u8();
    return buf;
}

i32 main() {
    
    string* s = $string* gen_trash(sizeof(string));
    new (s) string("asdf");
    s->~();
    free($void* s, sizeof(string));

    return 0;
}